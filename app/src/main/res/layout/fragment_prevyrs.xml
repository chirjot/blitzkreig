<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bb5"
    android:layout_height="match_parent"
    tools:context="com.example.acer.firstapp.PrevyrsFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:text="Sessional 1"
            android:textColor="#000"
            android:layout_marginTop="54sp"
            android:layout_marginLeft="20sp"
            android:textSize="25sp"
            android:id="@+id/tvsess1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:text="Sessional 2"
            android:textColor="#000"
            android:layout_marginTop="30sp"
            android:layout_marginLeft="20sp"
            android:textSize="25sp"
            android:id="@+id/tvsess2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:text="End Term"
            android:textColor="#000"
            android:layout_marginTop="30sp"
            android:layout_marginLeft="20sp"
            android:textSize="25sp"
            android:id="@+id/tvend3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageButton
                android:layout_marginLeft="250sp"
                android:layout_marginRight="20sp"
                android:layout_marginTop="45sp"
                android:id="@+id/ibdownload12"
                android:src="@drawable/download"
                android:onClick="Click"
                android:scaleType="fitCenter"
                style="@style/btnstyle1"
                android:layout_width="50sp"
                android:layout_height="50sp" />

            <ImageButton
                android:layout_marginTop="45sp"
                android:id="@+id/ibpreview12"
                android:src="@drawable/preview"
                android:scaleType="fitCenter"
                android:onClick="Click"
                style="@style/btnstyle1"
                android:layout_width="50sp"
                android:layout_height="50sp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageButton
                android:layout_marginLeft="250sp"
                android:layout_marginRight="20sp"
                android:layout_marginTop="15sp"
                android:id="@+id/ibdownload22"
                android:src="@drawable/download"
                android:scaleType="fitCenter"
                android:onClick="Click"
                style="@style/btnstyle1"
                android:layout_width="50sp"
                android:layout_height="50sp" />

            <ImageButton
                android:layout_marginTop="15sp"
                android:id="@+id/ibpreview22"
                android:src="@drawable/preview"
                android:scaleType="fitCenter"
                style="@style/btnstyle1"
                android:onClick="Click"
                android:layout_width="50sp"
                android:layout_height="50sp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageButton
                android:layout_marginLeft="250sp"
                android:layout_marginRight="20sp"
                android:layout_marginTop="15sp"
                android:id="@+id/ibdownload32"
                android:onClick="Click"
                android:src="@drawable/download"
                android:scaleType="fitCenter"
                style="@style/btnstyle1"
                android:layout_width="50sp"
                android:layout_height="50sp" />

            <ImageButton
                android:layout_marginTop="15sp"
                android:id="@+id/ibpreview32"
                android:src="@drawable/preview"
                android:scaleType="fitCenter"
                android:onClick="Click"
                style="@style/btnstyle1"
                android:layout_width="50sp"
                android:layout_height="50sp" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
